{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "es2019",
	  "noImplicitAny": false,
	  "sourceMap": true,
	  "outDir": "build",
	  "emitDecoratorMetadata": true,
	  "experimentalDecorators": true,
	  "importHelpers": true,
	  "forceConsistentCasingInFileNames": true,
	  "lib": ["es2019", "esnext.asynciterable"],
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "skipLibCheck": true
	},
	"exclude": ["node_modules"]
  }
  